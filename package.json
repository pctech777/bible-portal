{
  "name": "bible-portal",
  "version": "1.5.1",
  "description": "Comprehensive Bible study plugin for Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "npm run copy-static && node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "copy-static": "cp styles.css manifest.json /home/stephen/ecosystem/dev-vault/.obsidian/plugins/bible-portal/",
    "deploy": "cp main.js styles.css manifest.json /home/stephen/ecosystem/dev-vault/.obsidian/plugins/bible-portal/ && cp -r data /home/stephen/ecosystem/dev-vault/.obsidian/plugins/bible-portal/",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "bible",
    "bible-study"
  ],
  "author": "Stephen",
  "license": "MIT",
  "devDependencies": {
    "@eslint/json": "^0.14.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@typescript-eslint/utils": "^8.51.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "typescript-eslint": "^8.51.0"
  }
}
